{
  "tabWidth": 2,
  "printWidth": 120,
  "endOfLine": "lf",
  "semi": true,
  "singleQuote": false,
  "trailingComma": "none",
  "arrowParens": "avoid",
  "cssDeclarationSorterKeepOverrides": false,
  "importOrder": [
    "^@/(.*)$",
    "^@electron/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-css-order"
  ]
}
